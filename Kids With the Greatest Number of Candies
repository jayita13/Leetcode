https://leetcode.com/problems/kids-with-the-greatest-number-of-candies/

def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:
        maxi = max(candies)
        out = []
        for i in range(0,len(candies)):
            if(candies[i] + extraCandies >= maxi) :
                out.append(True)
            else:
                out.append(False)
        return out  
